{
  "configurations": [
    {
      "name": "Launch Chrome",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:8080/",
      "webRoot": "${workspaceFolder}/dev-client",
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/src/*"
      },
      "runtimeArgs": ["--auto-open-devtools-for-tabs"]
    }
  ]
}
